   "source": [
    "from collections import Counter\n",
    "\n",
    "class Solution:\n",
    "    def uniqueOccurrences(self, arr):\n",
    "        counter = Counter(arr)\n",
    "        return len(counter) == len(set(counter.values()))"
   ]
